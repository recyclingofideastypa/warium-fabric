{
  "multipart": [
    {
      "apply": {
        "model": "crusty_chunks:block/rebar_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "crusty_chunks:block/rebar_noside",
        "y": 270
      }
    }
  ]
}